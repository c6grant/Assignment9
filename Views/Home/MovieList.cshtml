@{
    ViewData["Title"] = "Movie List";

}
@model IEnumerable<Assignment3.Models.InputResponse>  

<div>
    <h2>Movie List</h2>

    <table>
        <thead>
            <tr><th>Category</th><th>Title</th><th>Year</th><th>Director</th><th>Rating</th><th>Edited</th><th>Lent To</th><th>Notes</th></tr>
        </thead>

        <tbody>
            @foreach (Assignment3.Models.InputResponse x in Model)
            {
            <tr>
                <td>@x.Category</td>
                <td>@x.Title</td>
                <td>@x.Year</td>
                <td>@x.Director</td>
                <td>@x.Rating</td>
                <td>@x.Edited</td>
                <td>@x.LentTo</td>
                <td>@x.Notes</td>
            </tr>
            }
        </tbody>


    </table>
</div>
